<main>
    <section>
        <div class="p-1 p-3">
            {{#with navigation}}
            <div class="card">
            <div class="nav">
             {{#each links}}
             <a href="{{href}}" class="nav-link {{#unless href}} disabled {{/unless}}">{{text}}</a>
             {{/each}}
            </div>
            </div>
            {{/with}}
        </div>
    </section>

    <section>
     <div class="row p-3">
     <div class="col-8">
     <div class="p-4">
      <h1>{{header}}</h1>
       <div class="pt-5">
        {{#with contacts_data}}
         <h3>{{title}}</h3>
          <div class="pb-3">
            {{#each buttons}}
             <button class="btn {{#if isPrimary}}btn-primary{{/if}} {{#if isOutline}}btn-outline-primary{{/if}}">{{text}}</button>
            {{/each}}
           </div>

            <div class="row">{{#with form}}
                 {{#with input}}
                  <div class="col-6">
                    {{#with surname}}
                     <label class="form-label">{{label}}</label>
                     <div><input class="form-control" placeholder="{{placeholder}}"></div>
                     <div class="form-text">{{caption}}</div>
                    {{/with}}
                   </div>
                    <div class="col-6">
                        {{#with name}}
                         <label class="form-label">{{label}}</label>
                         <div><input class="form-control" placeholder="{{placeholder}}"></div>
                         <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                     <div class="col-6">
                        {{#with phone}}
                         <label class="form-label">{{label}}</label>
                         <div><input class="form-control" placeholder="{{placeholder}}"></div>
                         <div class="form-text">{{caption}}</div>
                        {{/with}}
                        </div>
                         <div class="col-6 pb-3">
                            {{#with email}}
                             <label class="form-label">{{label}}</label>
                             <div><input class="form-control" placeholder="{{placeholder}}"></div>
                             <div class="form-text">{{caption}}</div>
                            {{/with}}
                         </div>
                        {{/with}}
                        <hr>
                   
    <div class="pb-2">{{#each switch}}
    <div class="form-check form-switch">
    <label class="form-check-label">{{text}}</label>
    <input {{#if isActive}}checked{{/if}} type="checkbox" class="form-check-input">
    </div>
    {{/each}}
    </div>
    <hr>
    {{/with}}
    </div>
    {{/with}}
    </div>
    </div>
    </div>

    <div class="col-4 p-5 ps-0">
    <section class="card">
    {{#each action_block}}
    <div class="p-2">
    <button class="btn {{#if isSuccess}}btn-success{{/if}}
    {{#if isDanger}}btn-danger{{/if}}
    {{#if isInfo}}btn-info{{/if}}">{{text}}</button>
    </div>
    {{/each}}
    </section>

    <section class="card">
    {{#with total}}
    <h2 class="m-2">{{title}}</h2>
    <div class="m-2">
    {{#each item_list}}
    <div>{{name}}:{{price}}</div>
    {{/each}}
    </div>
    <hr class="mt-0">
    <div class="m-2">
    {{#each additional_info}}
    <div>{{text}}:{{value}}</div>
    {{/each}}
    </div>
    <hr class="mt-0">
    <div class="m-2">{{#with total_price}}
    <div>{{text}}:{{value}}</div>
    {{/with}}
    </div>
    <hr class="mt-0">
    <div class="m-2">
    {{#each button}}
    <div class="m-2"><button class="btn {{#if isPrimary}}btn-primary{{/if}} {{#if isOutline}}btn-outline-primary{{/if}}">{{text}}</button></div>
    {{/each}}
    </div>
    {{/with}}
    </section>
    </div>

    <section>
    <div>
    {{#with delivery}}
    <h2>{{title}}</h2>
    <div class="card">
    <div class="row">
