    <div class="form-text">{{caption}}</div>
    {{/with}}
    </div>
    </div>
    {{/with}}
    </div>
    <hr class="pt-0">
    <div class="p-2">
    {{#with coment}}
    <label class="form-label"><h4>{{label}}</h4></label>
    <div><input placeholder="{{placeholder}}" class="form-control"></div>
    {{/with}}
