<main>
    <section>
        <div><h1>{{name}}</h1></div>
        <div><time>{{birthdate}}</time></div>
        <div><address>{{birthplace}}</address></div>
        <div><time>{{deathdate}}</time></div>
        <div><em>{{deathplace}}</em></div>
        <div>{{nationality}}</div>
        <div>{{occupation}}</div>
        <div><mark>known_for:</mark>
            <ul>
            {{#each known_for}}
            <li>{{this}}</li>
            {{/each}}
            </ul>
        </div>
    <hr> 
    </section>

    <section>
        {{> title data='My education' big=true}}
  <div>
    <ul>
        {{#each education}}
        <li>{{degree}}</li>
        <li>{{field}}</li>
        <li>{{institution}}</li>
        <li><time>{{year}}</time></li>
        {{/each}}
   </ul>
  </div>
  <hr>
    </section>

   <section>
  <p>
    {{> title data='Notable_publications' big=true}}
    <ul>
        {{#each notable_publications}}
        <li><b>title:</b> {{title}}</li>
        <li><b>year:</b> <time>{{year}}</time></li>
        <li><b>publisher:</b> {{publisher}}</li>
        {{/each}}
    </ul>
  </p>
  <hr>
  </section>

  <section>
   {{> title data='Partners' big=true}} 
   <div>
    <ul>
    {{#each partners}}
      <li><b>name:</b> {{name}}</li>
      <li><b>relationship:</b> {{relationship}}</li>
      <li><b>years:</b> <time>{{years}}</time></li>
    {{/each}}
    </ul>
    <br>

    {{> title data='Awards' big=true}}
    <div>
    <ul>
        {{#each awards}}
        <li>{{title}}</li>
        <li>{{year}}</li>
        <li>{{description}}</li>
        {{/each}}
    </ul>
    <br>

    {{> title data='Influences' big=true}}
    <ul>
    {{#each influences}}
    <li>{{this}}</li>
    {{/each}}
    </ul>
    <br>

    {{> title data='Influenced' big=true}}
    <ul>
    {{#each influenced}}
    <li>{{this}}</li>
    {{/each}}
    </ul>
    <br>

    {{> title data='Quotes' big=true}}
    <ul>
        {{#each quotes}}
        <li><q>{{this}}</q></li>
        {{/each}}
    </ul>
  </div>
 </section>

 <section>
    {{> title data='Major_discoveries' big=true}}
    <div>
     <ul>
        {{#each major_discoveries}}
        <li><mark>title:</mark> {{title}}</li>
        <li><mark>year:</mark> {{year}}</li>
        <li><mark>description:</mark> {{description}}</li>
        {{/each}}
     </ul>
     <br>

     {{> title data='Contributions' big=true}}
       {{#with contributions}}
       <div><b>title:</b> {{title}}</div>
       <div><b>description:</b> <blockquote>{{description}}</blockquote></div>
       <div><b>year:</b> {{year}}</div>
       <div><b>field:</b> {{field}}</div>
        {{/with}}
  </div>
 </section>
</main>